<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip code</th>
      <th>County</th>
      <th>Owner name</th>
      <th>Auction amount</th>
      <th>Arv</th>
      <th>Property type</th>
      <th>Number of bedrooms</th>
      <th>Number of bathrooms</th>
      <th>Home sqr footage</th>
      <th>Property sqr footage</th>
      <th>Found by</th>
      <th>Secondary revision</th>
      <th>Type of loan</th>
      <th>Home status</th>
      <th>Notes</th>
      <th>Agent</th>
      <th>Review by date</th>
      <th>Urgent</th>
      <th>Possible phone numbers</th>
      <th>Possible address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.address %></td>
        <td><%= property.city %></td>
        <td><%= property.state %></td>
        <td><%= property.zip_code %></td>
        <td><%= property.county %></td>
        <td><%= property.owner_name %></td>
        <td><%= property.auction_amount %></td>
        <td><%= property.arv %></td>
        <td><%= property.property_type %></td>
        <td><%= property.number_of_bedrooms %></td>
        <td><%= property.number_of_bathrooms %></td>
        <td><%= property.home_sqr_footage %></td>
        <td><%= property.property_sqr_footage %></td>
        <td><%= property.found_by %></td>
        <td><%= property.secondary_revision %></td>
        <td><%= property.type_of_loan %></td>
        <td><%= property.home_status %></td>
        <td><%= property.notes %></td>
        <td><%= property.agent %></td>
        <td><%= property.review_by_date %></td>
        <td><%= property.urgent %></td>
        <td><%= property.possible_phone_numbers %></td>
        <td><%= property.possible_address %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
